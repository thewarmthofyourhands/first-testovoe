{
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "ext-curl": "*",
        "ext-pdo": "*",
        "eva/env": "dev-main",
        "eva/event-dispatcher": "dev-main",
        "eva/console": "dev-main",
        "eva/dependency-injection": "dev-main",
        "eva/foundation": "dev-main",
        "eva/common": "dev-main",
        "eva/filesystem": "dev-main"
    },
    "autoload": {
        "files": [
            "src/functions/helpers.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
